{
  "name": "effect-sugar-transform",
  "version": "0.1.0",
  "description": "Source transformer for Effect-TS gen block syntax",
  "type": "module",
  "main": "./target/dist/index.js",
  "types": "./target/dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "target/dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf target/dist",
    "version:dev": "npm version --no-git-tag-version \"$(node -p \"require('./package.json').version.split('-')[0]\")-dev.$(date +%Y%m%d%H%M%S)\"",
    "publish:dev": "pnpm build && pnpm version:dev && pnpm publish --registry http://localhost:4873 --no-git-checks && git checkout package.json"
  },
  "keywords": [
    "effect-ts",
    "effect",
    "transform",
    "functional-programming",
    "monads",
    "for-comprehension",
    "gen-blocks"
  ],
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "@babel/template": "^7.23.0",
    "@types/babel__core": "^7.20.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clayroach/effect-sugar.git",
    "directory": "packages/transform"
  },
  "license": "MIT"
}
