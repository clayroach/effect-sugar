{
  "name": "babel-plugin-effect-sugar",
  "version": "0.1.0",
  "description": "Babel plugin for Effect-TS syntactic sugar with for-comprehension and <- operator",
  "type": "module",
  "main": "./target/dist/index.js",
  "types": "./target/dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "target/dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf target/dist"
  },
  "keywords": [
    "babel-plugin",
    "effect-ts",
    "functional-programming",
    "monads",
    "for-comprehension"
  ],
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "@babel/template": "^7.23.0",
    "@types/babel__core": "^7.20.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clayroach/effect-sugar.git",
    "directory": "packages/babel-plugin"
  },
  "license": "MIT"
}
